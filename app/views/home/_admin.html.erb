		<table class="table table-condensed table-striped">
		<b> Assigned Customers</b><hr>
			<tr> 
				<th>Customer Name</th>
				<th>Type </th>
				<th>Status</th>
				<th>Assiged Tab</th>
			</tr>
			<% @customer_verifications.each do |verification| %>
			<tr>
				<td><%= verification.customer.slug %></td>
				<td><%= verification.customer.verification_type.name %></td>
				<td><h6 class="label label-danger"><%= verification.customer.status.humanize %></h6></td>
				<td><%= verification.tab.name.humanize %></td>
			</tr>
		<% end %>
		</table>